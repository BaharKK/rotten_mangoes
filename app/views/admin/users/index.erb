<%= link_to "Back to movie", movie_path(@movie) %><br/>
<h1> Users </h1>

<table class="table" id="users"> 
<thead>
  <tr>
    <th> Full Name </th>
    <th> Email </th>
    <th> Admin </th>
    <th> Created At </th>
    <th> Updated At </th>
  </tr>
</thead>
<tbody>
  <% @users.each do |user| %> 
    <tr> 
      <td><%= link_to user.full_name, admin_user_path(user) %></td>
      <td><%= user.email %></td>
      <td><%= user.admin %></td>
      <td><%= user.created_at.strftime("%B %e, %Y") %></td>
      <td><%= user.updated_at.strftime("%B %e, %Y") %></td>
    </tr>
  <% end %>
</tbody>
</table>



<%= paginate @users %>
