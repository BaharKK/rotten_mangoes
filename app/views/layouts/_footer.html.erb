<footer>
  <small>
    <% if current_user %>
      <% if current_user.admin %>
       <h6> Admin <%= current_user.full_name %><br><%= link_to "log out", session_path("current"), method: :delete %> | 
          <%= link_to "Admin User Management", admin_users_path %> | <%= link_to "Create A User", new_admin_user_path %> </h6>
      <% else %>
        Signed in as <%= current_user.full_name %> (<%= link_to "Log out", session_path("current"), method: :delete %>)
